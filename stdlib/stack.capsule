local Array = self:import("stdlib/array")

self.new = function(self)
  return setmetatable({
    __type = "stack",
    _array = Array:new(),
    push = function(self, value)
      return self._array:push(value)
    end,

    pop = function(self)
      return self._array:pop()
    end,

    toString = function(self)
      return self._array:toString()
    end,
  }, {
    __index = function(self)
      return nil
    end,

    __newindex = function(self)
      return nil
    end
  })
end