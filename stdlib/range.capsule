this.between = function(this, startPoint, endPoint)
  local rangeTable = {}
  for i = startPoint, endPoint do
    table.insert(rangeTable, i)
  end

  unpack(rangeTable)
end

this.to = function(this, endPoint)
  local rangeTable = {}
  for i = 1, endPoint do
    table.insert(rangeTable, i)
  end

  unpack(rangeTable)
end